#: .\Lib\site-packages\django\contrib\flatpages\admin.py:12
msgid "Advanced options"
msgstr "Pokročilé nastavenia"

#: .\Lib\site-packages\django\contrib\flatpages\apps.py:7
msgid "Flat Pages"
msgstr "Statické stránky"

#: .\Lib\site-packages\django\contrib\flatpages\forms.py:9
#: .\Lib\site-packages\django\contrib\flatpages\models.py:12
msgid "URL"
msgstr "URL"

#: .\Lib\site-packages\django\contrib\flatpages\forms.py:12
msgid ""
"Example: '/about/contact/'. Make sure to have leading and trailing slashes."
msgstr ""
"Príklad: '/about/contact/'. Uistite sa, že máte lomky na začiatku a na konci."

#: .\Lib\site-packages\django\contrib\flatpages\forms.py:15
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Táto hodnota musí obsahovať len písmená, číslice, bodky, podčiarovníky, "
"pomlčky, lomky alebo vlnovky."

#: .\Lib\site-packages\django\contrib\flatpages\forms.py:29
msgid "URL is missing a leading slash."
msgstr "V URL chýba úvodná lomka."

#: .\Lib\site-packages\django\contrib\flatpages\forms.py:37
msgid "URL is missing a trailing slash."
msgstr "V URL chýba koncová lomka."

#: .\Lib\site-packages\django\contrib\flatpages\forms.py:54
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr "Statická stránka s url %(url)s pre sídlo %(site)s už existuje."

#: .\Lib\site-packages\django\contrib\flatpages\models.py:13
msgid "title"
msgstr "názov"

#: .\Lib\site-packages\django\contrib\flatpages\models.py:14
msgid "content"
msgstr "obsah"

#: .\Lib\site-packages\django\contrib\flatpages\models.py:15
msgid "enable comments"
msgstr "povoliť komentáre"

#: .\Lib\site-packages\django\contrib\flatpages\models.py:17
msgid "template name"
msgstr "názov šablóny"

#: .\Lib\site-packages\django\contrib\flatpages\models.py:21
msgid ""
"Example: 'flatpages/contact_page.html'. If this isn't provided, the system "
"will use 'flatpages/default.html'."
msgstr ""
"Príklad: 'flatpages/contact_page.html'. Ak nič nenapíšete, systém použije "
"'flatpages/default.html'."

#: .\Lib\site-packages\django\contrib\flatpages\models.py:26
msgid "registration required"
msgstr "nutná registrácia"

#: .\Lib\site-packages\django\contrib\flatpages\models.py:27
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr ""
"Ak je toto zaškrtnuté, stránku budú môcť prezerať len prihlásení "
"používatelia."

#: .\Lib\site-packages\django\contrib\flatpages\models.py:30
msgid "sites"
msgstr ""

#: .\Lib\site-packages\django\contrib\flatpages\models.py:34
msgid "flat page"
msgstr "statická stránka"

#: .\Lib\site-packages\django\contrib\flatpages\models.py:35
msgid "flat pages"
msgstr "statické stránky"
